{
  "name": "axiom",
  "version": "1.0.0",
  "description": "A lightweight cross-platform Digital Asset Management app",
  "main": "dist/main.js",
  "scripts": {
    "start": "npm run build:main && npm run build:preload && concurrently \"npm run watch:renderer\" \"npm run start:electron\"",
    "watch:renderer": "webpack serve --config webpack.renderer.config.js",
    "build:main": "webpack --config webpack.main.config.js",
    "build:renderer": "webpack --config webpack.renderer.config.js",
    "build:preload": "tsc src/main/preload.ts --outDir dist",
    "build": "npm run build:main && npm run build:preload && npm run build:renderer",
    "start:electron": "wait-on http://localhost:9000 && electron .",
    "package": "npm run build && electron-builder"
  },
  "keywords": [
    "electron",
    "react",
    "typescript",
    "dam"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "electron": "^32.2.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "concurrently": "^9.2.1",
    "css-loader": "^7.1.2",
    "electron-builder": "^25.1.8",
    "html-webpack-plugin": "^5.6.3",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.7.2",
    "wait-on": "^8.0.5",
    "webpack": "^5.96.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.1.0"
  }
}
